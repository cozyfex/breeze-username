{
    "name": "cozyfex/breeze-username",
    "description": "Login with username in Laravel Breeze",
    "keywords": [
        "breeze",
        "username"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/cozyfex/laravel-breeze-username/issues",
        "source": "https://github.com/cozyfex/laravel-breeze-username"
    },
    "authors": [
        {
            "name": "Felix D.H. Kang",
            "email": "cozyfex@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0.0",
        "laravel/breeze": "^1.1.8"
    },
    "autoload": {
        "psr-4": {
            "CozyFex\\BreezeUsername\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "laravel": {
            "providers": [
                "CozyFex\\BreezeUsername\\BreezeUsernameServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
